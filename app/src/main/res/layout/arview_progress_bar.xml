<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <ProgressBar
        android:id="@+id/progressBarAbono"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="40dp"
        android:progress="40" />

    <ProgressBar
        android:id="@+id/progressBarWater"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="7dp"
        android:progress="50" />

    <ImageView
        android:id="@+id/barIconWater"
        android:layout_width="27dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/progress_bar_icon_water" />

    <ImageView
        android:id="@+id/barIconAbono"
        android:layout_width="21dp"
        android:layout_height="17dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/progress_bar_icon_abono" />


</RelativeLayout>

